{
    "name": "inanepain/http",
    "description": "Http client, request and response objects implementing psr-7 (message interfaces).",
    "type": "library",
    "license": "Unlicense",
    "homepage": "https://github.com/inanepain/http",
    "readme": "README.md",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "philip@cathedral.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "philip@cathedral.co.za",
        "issues": "https://github.com/inanepain"
    },
    "keywords": [
        "inane",
        "http",
        "request",
        "response",
        "psr-7"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Http\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "psr/http-message": "*",
        "psr/http-client": "*",
        "inanepain/file": ">=0.1.0 || dev-master || dev-develop",
        "inanepain/stdlib": ">=0.1.4 || dev-master || dev-develop"
    },
    "require-dev": {}
}
